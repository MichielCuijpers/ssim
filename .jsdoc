{
	"tags": {
		"allowUnknownTags": false,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"includePattern": ".+\\.js$",
		"exclude": [
			"dist/",
			"docs/",
			"ci/",
			"cli/",
			"coverage",
			"node_modules",
			"schema",
			"spec"
		]
	},
	"plugins": [],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"private": true
	}
}